    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "*",
    "nyc": "*",
    "prettier": "1.11.1",
    "pretty-quick": "^1.4.1",
    "rollup": "^0.57.1",
