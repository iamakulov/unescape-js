{
  "name": "unescape-js",
  "version": "1.1.0",
  "description": "Unescape JavaScript special characters",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamakulov/unescape-js.git"
  },
  "bugs": {
